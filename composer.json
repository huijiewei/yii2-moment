{
  "name": "huijiewei/yii2-moment",
  "description": "A lightweight JavaScript date library for parsing, validating, manipulating, and formatting dates.",
  "minimum-stability": "stable",
  "license": "MIT",
  "type": "yii2-extension",
  "homepage": "https://github.com/huijiewei/yii2-moment.git",
  "keywords": [
    "yii2",
    "extension",
    "moment",
    "momentjs",
    "assets"
  ],
  "authors": [
    {
      "name": "Huijie Wei",
      "email": "huijiewei@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0.2",
    "yiisoft/yii2": "^2.0",
    "npm-asset/moment": "^2.29.4"
  },
  "autoload": {
    "psr-4": {
      "huijiewei\\moment\\": "src"
    }
  },
  "repositories": {
    "asset": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
